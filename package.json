{
  "name": "jquery-interactiveTimelineExceedanceModule",
  "version": "0.1.0",
  "description": "Interactive Timeline Exceedance Module",
  "keywords": [
    "jquery-plugin",
    "jquery",
    "interactive timeline exceedance module",
    "plugins"
  ],
  "author": {
    "name": "Josh Wilson",
    "email": "jwilson@waystone.systems"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "autoprefixer": "~6.7.6",
    "jspm": "^0.16.52",
    "node-sass": "~4.5.0",
    "postcss-cli": "~3.0.0-beta",
    "postcss-flexbugs-fixes": "~2.1.0"
  },
  "scripts": {
    "build": "npm run build-js  && npm run build-scss && npm run build-demo-js && npm run build-demo-html",
    "build-js": "jspm bundle-sfx src/jquery.fl-item.js dist/jquery.fl-item.js && jspm bundle-sfx src/jquery.fl-item.js dist/jquery.fl-item.min.js --minify",
    "build-demo-js": "jspm bundle-sfx src/demo.js demo/demo.js --global-name Demo --skip-source-maps",
    "build-demo-html": "perl -0777 -pe 's/<!--dev environment-->(.*)<!--end dev-->.*<!-- uncomment for dist(.*?)-->/$2/s' src/index.html > demo/index.html",
    "build-scss": "node-sass src/scss/*.scss -o dist/ && npm run build-autoprefix-css",
    "build-autoprefix-css": "postcss dist/*.css --use autoprefixer postcss-flexbugs-fixes --replace",
    "postinstall": "jspm install"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "scss": "github:KevCJones/plugin-scss@^0.2.11"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "chart.js": "npm:chart.js@^2.5.0",
      "core-js": "npm:core-js@^1.1.4",
      "jquery": "npm:jquery@^3.1.1",
      "jquery-ui": "npm:jquery-ui@^1.12.1",
      "lodash": "npm:lodash@^4.17.4",
      "regression": "npm:regression@^1.4.0"
    }
  }
}
