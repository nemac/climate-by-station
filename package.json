{
  "name": "Climate by Station",
  "version": "0.0.1",
  "description": "Climate data from weather stations around the U.S.",
  "keywords": [
    "jquery-plugin",
    "jquery",
    "interactive timeline exceedance module",
    "plugins"
  ],
  "author": {
    "name": "Josh Wilson",
    "email": "jwilson@fernleafinteractive.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:js && npm run build:html",
    "build:js": "mkdir -p dist && rollup -c --environment=\"build_env:prod\"",
    "build:html": "rsync src/index.html dist/",
		"deploy:staging": "aws s3 sync --delete --no-follow-symlinks dist/ s3://climate-by-station.fernleafinteractive.com/"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/plugin-transform-template-literals": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "lodash-es": "^4.17.21",
    "rollup": "^2.58.0"
  }
}
